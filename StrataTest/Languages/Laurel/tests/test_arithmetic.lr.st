procedure main() {
    var x: int := 5;
    var y: int := 3;

    var prod: int := x * y;
    assert prod == 15;

    var a: int := 10;
    var b: int := 2;
    var result: int := a * b;
    assert result == 20;

    var sum_val: int := x + y;
    assert sum_val == 8;

    var diff: int := x - y;
    assert diff == 2;
}
